{
    "name": "digilopment/n8n-client",
    "description": "PHP client library for n8n webhook integration",
    "type": "library",
    "keywords": ["n8n", "webhook", "api", "client"],
    "license": "MIT",
    "authors": [
        {
            "name": "Digilopment",
            "email": "info@digilopment.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "vlucas/phpdotenv": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Digilopment\\N8NClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Digilopment\\N8NClient\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit"
    }
}
